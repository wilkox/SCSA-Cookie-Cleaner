{
  "manifest_version": 3,
  "name": "Cookie Cleaner for b2clogin.com",
  "version": "1.0",
  "description": "Clears cookies for b2clogin.com with a single click",
  "permissions": [
    "cookies",
    "tabs"
  ],
  "host_permissions": [
    "*://*.b2clogin.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}